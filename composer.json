{
    "name": "gushphp/gush",
    "type": "library",
    "description": "Project Maintenance & Contributing Automation Tool",
    "keywords": ["workflow", "issue", "tracker", "adapter", "pull request"],
    "homepage": "http://gushphp.org",
    "license": "MIT",
    "authors": [{ "name": "makers", "homepage": "https://github.com/gushphp/gush/graphs/contributors" }],
    "require": {
        "php": ">=5.4",
        "ext-curl": "*",
        "symfony/console": "~2.7.0@dev",
        "symfony/process": "~2.6.0",
        "symfony/filesystem": "~2.6.0",
        "symfony/finder": "~2.6.0",
        "symfony/yaml": "~2.6.0",
        "symfony/event-dispatcher": "~2.6.0",
        "knplabs/github-api": "~1.3.1",
        "ddd/slug": "~1.0.0",
        "herrera-io/version": "~1.1.1",
        "kherge/amend": "~3.0.0",
        "ramsey/array_column": "~1.1.2",
        "gushphp/gush-github-adapter": "~2.0@dev",
        "gushphp/gush-bitbucket-adapter": "~2.0@dev",
        "gushphp/gush-gitlab-adapter": "~2.0@dev",
        "gushphp/gush-jira-adapter": "~2.0@dev"
    },
    "require-dev": {
        "raulfraile/ladybug": "~1.0.8",
        "mikey179/vfsStream": "~1.5.0"
    },
    "autoload": {
        "psr-4": {"Gush\\": "src"}
    },
    "autoload-dev": {
        "psr-4": {"Gush\\Tests\\": "tests"}
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
