{
    "name": "gushphp/gush",
    "type": "library",
    "description": "Project Maintenance & Contributing Automation Tool",
    "keywords": ["workflow", "issue", "tracker", "adapter", "pull request"],
    "homepage": "http://gushphp.org",
    "license": "MIT",
    "authors": [{ "name": "makers", "homepage": "https://github.com/gushphp/gush/graphs/contributors" }],
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "guzzlehttp/guzzle": "^4.0",
        "guzzlehttp/progress-subscriber": "^1.1",
        "symfony/console": "^2.7.3",
        "symfony/process": "^2.7",
        "symfony/filesystem": "^2.7",
        "symfony/finder": "^2.7",
        "symfony/yaml": "^2.7",
        "symfony/event-dispatcher": "^2.7",
        "knplabs/github-api": "^1.3",
        "ddd/slug": "^1.0",
        "herrera-io/version": "^1.1",
        "kherge/amend": "^3.0",
        "ramsey/array_column": "^1.1.2",
        "gushphp/gush-github-adapter": "^1.2",
        "gushphp/gush-bitbucket-adapter": "^1.1.5",
        "gushphp/gush-gitlab-adapter": "^1.1.4",
        "gushphp/gush-jira-adapter": "^1.1.7"
    },
    "require-dev": {
        "mikey179/vfsStream": "^1.5"
    },
    "autoload": {
        "psr-4": {"Gush\\": "src"}
    },
    "autoload-dev": {
        "psr-4": {"Gush\\Tests\\": "tests"}
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.18-dev"
        }
    }
}
