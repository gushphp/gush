{
    "name": "gushphp/gush",
    "type": "library",
    "description": "Rapid workflow for project maintainers and contributors",
    "keywords": [
        "workflow", "git", "github", "console", "pull request",
        "issues", "maintainer", "repository", "automation", "robot",
        "contributor", "community", "foss", "gush", "package", "design"
    ],
    "homepage": "http://gushphp.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Luis Cordova",
            "email": "cordoval@gmail.com"
        },
        {
            "name" : "Daniel Gomes",
            "email": "me@danielcsgomes.com"
        },
        {
            "name": "Daniel T. Leech",
            "email": "dan.t.leech@gmail.com"
        },
        {
            "name": "Aaron Scherer",
            "email": "aequasi@gmail.com"
        },
        {
            "name": "Pierre du Plessis",
            "email": "pdples@gmail.com"
        },
       {
            "name": "Sebastiaan Stok",
            "email": "s.stok@rollercapes.net"
        }
    ],
    "require": {
        "php": ">=5.4",
        "ext-curl": "*",
        "symfony/console": "~2.4",
        "symfony/process": "~2.4",
        "symfony/filesystem": "~2.3",
        "symfony/yaml": "~2.3",
        "symfony/event-dispatcher": "~2.3",
        "knplabs/github-api": "~1.2",
        "ddd/slug": "~1.0",
        "herrera-io/version": "~1.0@dev",
        "kherge/amend": "~3.0",
        "symfony/property-access": "~2.4",
        "gushphp/gush-github-adapter": "dev-master@dev",
        "gushphp/gush-bitbucket-adapter": "dev-master@dev",
        "gushphp/gush-gitlab-adapter": "dev-master@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7",
        "raulfraile/ladybug": "~1.0"
    },
    "bin": [
        "bin/gush"
    ],
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {"Gush\\": "src"}
    },
    "autoload-dev": {
        "psr-4": {"Gush\\Tests\\": "tests"}
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        }
    }
}
